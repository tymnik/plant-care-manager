model Plant {
    id    Int     @id @default(autoincrement())
    name String
    scientificName String[]
    otherName String[]
    images String[]
    cycle String
    watering String
    depth_water_requirement String 
    volume_water_requirement String 
    watering_period String
    watering_general_benchmark String
    sunlight String[]
    pruning_month String[]
    pruningCount String
    flowers Boolean
    flowering_season String
    fruits Boolean
    cones Boolean
    fruiting_season String[]
    harvest_season String[] 
    harvest_method String[]
    leaf Boolean
    growth_rate String
    maintenance String
    medicinal Boolean
    edible_leaf Boolean
    poisonous_to_humans Boolean
    poisonous_to_pets Boolean
    drought_tolerant Boolean
    salt_tolerant Boolean
    thorny Boolean
    invasive Boolean
    cuisine Boolean
    indoor Boolean
    care_level String
    rare_level String
    rare Boolean
    description String
}
